<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:dct="http://purl.org/dc/terms/"
   xmlns:foaf="http://xmlns.com/foaf/0.1/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:vann="http://purl.org/vocab/vann/"
>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#QualityReport">
    <rdfs:label xml:lang="en">Quality Report</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:comment xml:lang="en">Represents a report on the problems detected during the assessment of quality on a dataset.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#problematicThing">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://purl.org/eis/vocab/qpro#QualityProblem"/>
    <rdfs:label xml:lang="en">Problematic Thing</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:comment xml:lang="en">Represent the actual problematic instance from the dataset. This could be a list of resources (rdf:Seq), a list of reified statements, or a model representing a list of statements.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#hasProblem">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:range rdf:resource="http://purl.org/eis/vocab/qpro#QualityProblem"/>
    <rdfs:domain rdf:resource="http://purl.org/eis/vocab/qpro#QualityReport"/>
    <rdfs:comment xml:lang="en">Identifies problem instances in the report</rdfs:comment>
    <rdfs:label xml:lang="en">has problem</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#QualityProblem">
    <rdfs:label xml:lang="en">Quality Problem</rdfs:label>
    <rdfs:comment xml:lang="en">Represents a quality problem detected during the assessment of quality metrics on triples.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#generatedBy">
    <rdfs:range rdf:resource="http://purl.org/eis/vocab/daq#Observation"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">generated by</rdfs:label>
    <rdfs:comment xml:lang="en">Links a quality problem to a particular observation in the metadata</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/eis/vocab/qpro#QualityReport"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#">
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-08-03</dct:modified>
    <foaf:homepage rdf:resource="http://purl.org/eis/vocab/qpro#"/>
    <dc:description xml:lang="en">The Quality Problem Report (QPRO) enables the fine-grained description of quality problems found while assessing a dataset.</dc:description>
    <vann:preferredNamespaceUri>http://purl.org/eis/vocab/qpro#</vann:preferredNamespaceUri>
    <vann:preferredNamespacePrefix>qpro</vann:preferredNamespacePrefix>
    <dct:title xml:lang="en">Quality Problem Report (QPRO)</dct:title>
    <dct:creator rdf:nodeID="ub66bL17C15"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
    <dct:language rdf:datatype="http://www.w3.org/2001/XMLSchema#language">en</dct:language>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#computedOn">
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdfs:label xml:lang="en">computed on</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://purl.org/eis/vocab/qpro#QualityReport"/>
    <rdfs:comment xml:lang="en">Represents the dataset URI on which quality metrics where computed</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#isDescribedBy">
    <rdfs:range rdf:resource="http://purl.org/eis/vocab/daq#Metric"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">is described by</rdfs:label>
    <rdfs:comment xml:lang="en">Represent the URI of the metric which instantiated this problem</rdfs:comment>
    <rdfs:domain rdf:resource="http://purl.org/eis/vocab/qpro#QualityProblem"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#inGraph">
    <rdfs:range rdf:resource="http://www.w3.org/2004/03/trix/rdfg-1Graph"/>
    <rdfs:domain rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Statement"/>
    <rdfs:comment xml:lang="en">This property enables the reification of quads by creating a new property to define the context of a triple statement.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">in graph</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#exceptionDescription">
    <rdfs:range rdf:resource="http://purl.org/eis/vocab/qpro#Exception"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#Literal"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Exception Description</rdfs:label>
    <rdfs:comment xml:lang="en">Describes a particular exception raised by the Resource or Triple in question, causing a quality problem</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#Exception">
    <rdfs:comment xml:lang="en">Represents an Exception that causes the dataset to have Quality Problems. This is an abstract class</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:label xml:lang="en">Exception (Abstract)</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/eis/vocab/qpro#extraExceptionProperty">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:comment xml:lang="en">Enables the description of extra exception properties. This is an abstract property.</rdfs:comment>
    <rdfs:label xml:lang="en">Extra Exception Property (Abstract)</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:nodeID="ub66bL17C15">
    <foaf:mbox rdf:resource="mailto:debattij@scss.tcd.ie"/>
    <foaf:name>Jeremy Debattista</foaf:name>
  </rdf:Description>
</rdf:RDF>